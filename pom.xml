        <dependency>
            <groupId>com.taobao</groupId>
            <artifactId>taobao-java-sdk-yestapi</artifactId>
            <version>1.0.0</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/src/main/resources/libs/top-api-sdk-dev-chendong-20150526.054721-12.jar</systemPath>
        </dependency>

