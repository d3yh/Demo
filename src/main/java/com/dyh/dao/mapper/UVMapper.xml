<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dyh.dao.UVMapper">


    <update id="queryNowCountByTime" parameterType="string">
        select nowcount from uv where clocktime = #{dateScore}
    </update>
</mapper>